{
    "root": true,
    "env": {
        "es2021": true,
        "browser": true
    },
    "extends": [
        "semistandard"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
        "space-before-function-paren": ["error", "never"]
    }
}
